{
  "name": "matimil-react-seed",
  "version": "1.0.1",
  "description": "A seed project for react js apps",
  "main": "client.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.debug.js --content-base dev --inline --hot",
    "build:prod": "webpack -p --progress --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.debug.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MatiMil/react-seed.git"
  },
  "dependencies": {
    "mobx": "^3.6.2",
    "mobx-react": "^4.4.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "webpack-cli": "^3.1.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.5",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "enzyme-to-json": "^3.3.4",
    "es6-promise": "^4.2.5",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "node-sass": "^4.10.0",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.6.3",
    "redux-logger": "^2.6.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-dev-server": "^3.0.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "keywords": [
    "react",
    "seed",
    "mobx",
    "js",
    "babel",
    "webpack"
  ],
  "author": "Matijas Milos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MatiMil/react-seed/issues"
  },
  "homepage": "https://github.com/MatiMil/react-seed#readme"
}
